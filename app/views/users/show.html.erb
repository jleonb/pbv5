<div class="container">
  
  <div id="slider_top">
    <% @slider_top.each do |slider_top| %>
      <a href="<%= slider_top.institucion.link %>"><img src="<%= slider_top.institucion.img.url(:large) %>" class="responsive-img"></a>
    <% end %>
  </div>

  <div class="center">
    <%= link_to estandars_path do %>
    <span class="waves-effect waves-light btn"><i class="material-icons left">person_pin</i>CONTACTAR EJECUTIVO</span>
    <% end %>
  </div>

</div>

<script type="text/javascript">
$(document).ready(function(){
$('#slider_top').slick({
autoplay: true,
autoplaySpeed: 2000,
arrows: false,
draggable: true,
mobileFirst: true,
responsive: true,
});
});
</script>