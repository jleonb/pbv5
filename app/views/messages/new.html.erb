<div class="container">
  <%= form_for @message, url: contact_path do |message_form| %>  
    <div class="field">
      <%= message_form.text_field :rut, placeholder: "Rut" %>
    </div>
    
    <div class="field">
      <%= message_form.text_area :content, placeholder: "Message"%>
    </div>
    <div class="field">
      <%= message_form.submit "Enviar" %>
    </div>
  <% end %>
</div>