<p id="notice"><%= notice %></p>

<h1>Listing Slider Resta</h1>

<table>
  <thead>
    <tr>
      <th>Img</th>
      <th>Link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @slider_resta.each do |slider_restum| %>
      <tr>
        <td><%= slider_restum.img %></td>
        <td><%= slider_restum.link %></td>
        <td><%= link_to 'Show', slider_restum %></td>
        <td><%= link_to 'Edit', edit_slider_restum_path(slider_restum) %></td>
        <td><%= link_to 'Destroy', slider_restum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Slider restum', new_slider_restum_path %>
